{
  "expo": {
    "name": "inhockey",
    "slug": "inhockey",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/inhockeypict.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maxsl77.inhockey"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/inhockeypict.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.maxsl77.inhockey"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "flipper": true
          }
        }
      ],
      "expo-font",
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "ec85a18b-3bbe-4ff3-89ea-a22a4fe4e65d"
      }
    }
  }
}
